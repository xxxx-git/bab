{
  "JWTSettings": {
    "Header": {
      "Type": "JWT",
      "Algorithm": "HS256"
    },
    "Payload": {
      "Issuer": "whoami",
      "Audience": "clients",
      "Subject": "MyTokenSubject",
      "Expires": {
        "Months": 0,
        "Days": 1,
        "Hours": 0
      },
      "Content": "user"
    },
    "Secret": "MySuperDuperSecretKey",
    "Http": {
      "Header": "Authorization",
      "Cookie": "Authorization",
      "HttpOnlyAccessCookie": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}